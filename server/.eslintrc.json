{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:@javascript-eslint/recommended",
        "prettier/@javascript-eslint",
        "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "never"
            }
          ],
          "prettier/prettier": "error"
        },
        "settings": {
            "import/resolver": {

            }
          }
}
